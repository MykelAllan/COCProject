{
  "name": "coc-project-cjqi",
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "client/build/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/players/:playerTag", "dest": "/api/players/:playerTag" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}